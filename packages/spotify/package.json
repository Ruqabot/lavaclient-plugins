{
    "name": "@lavaclient/spotify",
    "version": "3.0.0",
    "description": "A lavaclient plugin used for easy spotify integration.",
    "main": "dist/index.js",
    "types": "index.d.ts",
    "scripts": {
        "build": "rimraf dist && tsc",
        "lint": "eslint  --ext .ts",
        "lint:fix": "yarn lint --fix",
        "types": "tsc -d",
        "types:bundle": "dts-bundle --name @lavaclient/spotify --outputAsModuleFolder --main dist/index.d.ts --out ../index.d.ts --removeSource"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lavaclient/plugins.git"
    },
    "keywords": [
        "lavaclient",
        "lavalink"
    ],
    "author": "melike2d (edited by Ruzie)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/lavaclient/plugins/issues"
    },
    "homepage": "https://lavaclient.js.org/",
    "devDependencies": {
        "@lavaclient/queue": "^2.0.0-rc.1",
        "@types/node": "^16.0.0",
        "rimraf": "^3.0.2"
    },
    "dependencies": {}
}
